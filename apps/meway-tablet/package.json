{
  "private": true,
  "name": "@meway/meway-tablet",
  "version": "0.0.0",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "dev": "expo start",
    "build": "expo export --output-dir ./build --platform android",
    "start": "expo start",
    "android": "expo start --android",
    "web": "expo start --web",
    "eas-build-pre-install": "npm install --global pnpm@7.x",
    "eas-build-post-install": "pnpm run -w build:mobile"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.216.0",
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.216.0",
    "@meway/tailwind-config": "*",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "@react-navigation/stack": "^6.3.7",
    "aws-config": "^1.4.0",
    "aws-sdk": "^2.1261.0",
    "date-format": "^4.0.14",
    "expo": "^47.0.0",
    "expo-background-fetch": "^11.0.1",
    "expo-dev-client": "~2.0.0",
    "expo-file-system": "^15.1.1",
    "expo-linking": "^3.2.3",
    "expo-status-bar": "~1.4.2",
    "expo-task-manager": "^11.0.1",
    "expo-updates": "~0.15.4",
    "nativewind": "2.0.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.1.6",
    "@meway/eslint-config-custom": "*",
    "@types/react": "~18.0.25",
    "@types/react-dom": "~18.0.9",
    "@types/react-native": "~0.70.5",
    "autoprefixer": "^10.4.8",
    "babel-preset-expo": "~9.2.1",
    "postcss": "^8.4.19",
    "tailwindcss": "~3.2.1",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": "@meway/eslint-config-custom",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}