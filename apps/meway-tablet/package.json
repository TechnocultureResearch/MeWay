{
  "private": true,
  "name": "@meway/meway-tablet",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js,ts,tsx .",
    "dev": "expo start",
    "build": "expo export --output-dir ./build --platform android",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "web": "expo start --web",
    "eas-build-pre-install": "npm install --global pnpm@7.x",
    "eas-build-post-install": "pnpm run -w build:mobile",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/html-elements": "^0.2.2",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.8",
    "@realm/react": "^0.4.1",
    "expo": "~47.0.8",
    "expo-camera": "^13.1.0",
    "expo-dev-client": "~2.0.1",
    "expo-face-detector": "^12.0.0",
    "expo-image-picker": "^14.0.2",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "hermes-engine": "^0.11.0",
    "nativewind": "2.0.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-web": "~0.18.7",
    "realm": "^11.3.0",
    "tailwind-config": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.1.6",
    "@expo/webpack-config": "~0.17.2",
    "@types/react": "~18.0.25",
    "@types/react-dom": "~18.0.9",
    "@types/react-native": "~0.70.5",
    "autoprefixer": "^10.4.8",
    "babel-preset-expo": "~9.2.1",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.19",
    "postcss-loader": "^4.2.0",
    "tailwindcss": "~3.2.1",
    "typescript": "^4.7.4",
    "webpack": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-custom",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
